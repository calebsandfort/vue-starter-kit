{
  "name": "vue-starter-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "deploy": "npm run test:all && npm run build && npm run deploy:frontend && npm run deploy:backend",
    "babel-build:backend": "babel src/backend -d dist_backend/",
    "build:backend": "npm run remove:backend && npm run babel-build:backend && npm run copy:backend",
    "build:frontend": "vue-cli-service build",
    "copy:backend": "cp package.json dist_backend/ && cp .env dist_backend/",
    "deploy:backend": "pm2 deploy ecosystem.config.js production",
    "deploy:frontend": "vue-cli-service s3-deploy",
    "remove:backend": "rm -rf dist_backend/*",
    "rollup:backend": "npm run remove:backend && ./node_modules/.bin/rollup -c && npm run copy:backend",
    "start:backend": "nodemon --exec babel-node src/backend/index.js",
    "test:mocha": "mocha --reporter spec --require @babel/register --recursive \"src/**/*.spec.js\"",
    "test:mocha:debug": "mocha --reporter spec --inspect-brk --require @babel/register --recursive \"src/**/*.spec.js\"",
    "test:unit:debug": "node --inspect-brk ./node_modules/.bin/vue-cli-service test:unit --no-cache --runInBand",
    "test:all": "npm run test:mocha && npm run test:unit --no-cache --runInBand"
  },
  "dependencies": {
    "accounting-js": "^1.1.1",
    "apollo-boost": "^0.3.1",
    "apollo-link-ws": "^1.0.17",
    "apollo-server": "^2.4.8",
    "apollo-server-express": "^2.4.8",
    "axios": "^0.18.0",
    "big-integer": "^1.6.43",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.2",
    "console.table": "^0.10.0",
    "core-js": "^2.6.5",
    "cors": "^2.8.5",
    "cross-fetch": "^3.0.1",
    "dataloader": "^1.4.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "extend": "^3.0.2",
    "graphql": "^14.1.1",
    "graphql-iso-date": "^3.6.1",
    "jquery": "^3.4.1",
    "lodash": "^4.17.11",
    "material-design-icons-iconfont": "^3.0.3",
    "moment": "^2.24.0",
    "pg": "^7.10.0",
    "pg-hstore": "^2.3.2",
    "qs": "^6.7.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "roboto-fontface": "*",
    "sequelize": "^5.1.0",
    "subscriptions-transport-ws": "^0.9.16",
    "tor-request": "^3.1.0",
    "uuid": "^3.3.2",
    "v-debounce": "^0.1.2",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0-beta.28",
    "vue-cli-plugin-s3-deploy": "^3.0.0",
    "vue-router": "^3.0.3",
    "vuetify": "^1.5.5",
    "vuex": "^3.0.1",
    "x-frame-bypass": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.0.0",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-unit-jest": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "chai": "^4.2.0",
    "chai-exclude": "^1.0.12",
    "concurrently": "^4.1.0",
    "fs": "0.0.1-security",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.9",
    "pm2": "^3.5.0",
    "node-sass": "^7.0.0",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.0.5"
  }
}
